<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="12" />
    <option name="FONT_FAMILY" value="Fira Code" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Retina" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Retina" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="LiveEditOptions" nodeUpdateOnChanges="true" />
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>